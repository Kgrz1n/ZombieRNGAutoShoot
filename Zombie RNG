local mainBody = Instance.new("Frame", header)
mainBody.Size = UDim2.new(1, 0, 0, 370); mainBody.Position = UDim2.new(0, 0, 1, 1); mainBody.BackgroundColor3 = Color3.fromRGB(0, 0, 0); mainBody.BorderSizePixel = 0

-- Botão de Fechar (X)
local closeBtn = Instance.new("TextButton", header)
closeBtn.Size = UDim2.new(0, 25, 0, 25); closeBtn.Position = UDim2.new(1, -60, 0, 5); closeBtn.BackgroundColor3 = Color3.fromRGB(200, 50, 50); closeBtn.BorderSizePixel = 0; closeBtn.Text = "X"; closeBtn.TextColor3 = Color3.new(1, 1, 1); closeBtn.Font = Enum.Font.GothamBold; closeBtn.TextSize = 16; closeBtn.ZIndex = 10
closeBtn.MouseButton1Click:Connect(function()
    -- Restaurar gráficos antes de fechar
    if isLowGraphicsEnabled then
        toggleLowGraphics(false)
    end
    if isReduceMotionEnabled then
        toggleReduceMotion(false)
    end
    
    -- Destruir GUI
    screenGui:Destroy()
    
    -- Avisar que foi fechado
    print("[ZOMBIE RNG] Script fechado com sucesso!")
end)

